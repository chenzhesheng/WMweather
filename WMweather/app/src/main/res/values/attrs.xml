<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="DoubleTextView">
        <attr name="key_text" format="string|reference" />
        <attr name="value_text" format="string|reference" />
        <attr name="key_icon" format="reference" />
        <attr name="value_icon" format="reference" />
    </declare-styleable>

</resources>
